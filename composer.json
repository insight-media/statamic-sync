{
    "name": "insight-media/statamic-sync",
    "description": "A package for Statamic projects which provides console commands to synchronise content between local and remote projects.",
    "keywords": [
        "Insight Media",
        "laravel",
        "statamic-sync"
    ],
    "homepage": "https://github.com/insight-media/statamic-sync",
    "license": "MIT",
    "authors": [
        {
            "name": "Insight Media",
            "email": "dries@insight-media.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "spatie/ssh": "1.7"
    },
    "autoload": {
        "psr-4": {
            "InsightMedia\\StatamicSync\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "InsightMedia\\StatamicSync\\StatamicSyncServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
